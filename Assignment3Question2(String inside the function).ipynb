   "source": [
    "# String inside the function\n",
    "## Write a Python program to reverse a string.\n",
    "\n",
    "\n",
    "\n",
    "## Sample String : \"1234abcd\"\n",
    "\n",
    "## Expected Output : \"dcba4321\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6bc05339",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "output : dcba4321\n"
     ]
    }
   ],
   "source": [
    "# definition\n",
    "def reverse_string(s):\n",
    "    return s[::-1]\n",
    "\n",
    "# calling\n",
    "res= reverse_string(\"1234abcd\")\n",
    "print(\"output : {}\".format(res))"
   ]
  },
